% codigo 2 (obtencion de los porcentajes en base a rangos )
a = 0;
b = 0;
c = 0;
z = 0;

for i = 1:size(prectotland)
     x1 = prectotland(i)
     if (x1 >= 0.0) && (x1 <= 2.5)
      a = a+1
     end 
     if (x1 > 2.5) && (x1 <= 7.5)
      b = b+1
     end 
     if (x1 > 7.5) 
      c = c+1
     end 
end     

z = a+b+c

rangop1 = a/z
rangop2 = b/z
rangop3 = c/z
